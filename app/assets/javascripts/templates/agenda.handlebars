  <div class="agenda-container animated fadeInLeftBig">
      <div class="small-9 fc-container columns">
        {{full-calendar theEvents=model.events}}
      </div>

      <div class="ag-course-container small-3 columns">
        <h4>Agenda Courses</h4>

          <ul class="agenda-list">
           {{#each sect in controller.sections_list}}
            <li>
            {{sect.sect.course.courseNum}} - {{sect.course_title}}, Section {{sect.sectionNum}} w/ {{sect.sect.professor.fullName}} on {{sect.sect.daysOfWeek}} @ {{sect.sect.formatTimeSlot}}. CRN: {{sect.sect.crn}}  <a class="fancy-link" {{action "removeCourse" sect}}>X</a>
            </li>

          {{else}}
            Nothing has been added yet.
          {{/each}}
         </ul>


         <p>Currently working on ironing on the bugs for removing events from above and also working on saving the agenda for later viewing. Sorry for any inconvenience!</p>
      </div>

  </div>


{{outlet}}
