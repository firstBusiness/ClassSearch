{{!-- {{render "courses" model.courses}} --}}

<div  class="large-4 columns animated fadeInLeft course-container" id="courses-section">
<h1>Courses</h1>

  <ul class="common-list course-list">
  {{#each course in model.courses}}
    <li>{{#link-to 'course' course}}{{course.courseNum}} - {{course.title}}
    {{/link-to}}</li>
  {{/each}}
  </ul>
</div>

{{outlet}}